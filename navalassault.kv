<rootScreen>:
	BoxLayout:
		orientation: 'vertical'
		canvas.before:
			Color:
				rgb: (0, 0, 0)
			Rectangle:
				size: self.size
				pos: self.pos
		BoxLayout:
			size_hint_y: None
			height: '40dp'
			padding: 5
			Button:
				id: selectShip
				text: 'Select Ship'
				font_name: 'digital-7.regular.ttf'
				color: (0,1,0,1)
				font_size: '18dp'
				background_normal: 'btnMenuNormal.png'
				background_down: 'btnRadarFire.png'
			BoxLayout:
				Label:
					id: currentRank
					text: 'Rank:'
					color: (0,1,0,1)
					font_name: 'digital-7.regular.ttf'
					font_size: '25dp'
				Image:
					id: rankIcon
					source: 'seaman.png'
					allow_stretch: True
			BoxLayout:
				Button:
					id: mainMenu
					text: 'Main Menu'
					on_press: root.toMenu()
					color: (0,1,0,1)
					font_name: 'digital-7.regular.ttf'
					font_size: '18dp'
					background_normal: 'btnMenuNormal.png'
					background_down: 'btnRadarFire.png'
				Button:
					id: helpMenu
					text: 'Help'
					on_press: root.gotoHelp()
					color: (0,1,0,1)
					font_name: 'digital-7.regular.ttf'
					font_size: '18dp'
					background_normal: 'btnMenuNormal.png'
					background_down: 'btnRadarFire.png'
		BoxLayout:		
			FloatLayout:
				id: shipScreen
				canvas.before:
					Rectangle:
						size: self.size
						pos: self.pos
						source: 'Ocean.jpg'			

			FloatLayout:
				id: radarScreen
						
		BoxLayout:
			size_hint_y: None
			height: "40dp"
			Label:
				id: chgMultiPlayer1
				text: 'Computer:'
				size_hint_x: .15
				color: (0,1,0,1)
				font_name: 'digital-7.regular.ttf'
				font_size: '20dp'
			Label:
				id: cpuInfo
				size_hint_x: .35
				text: 'wait'
				color: (0,1,0,1)
				font_name: 'digital-7.regular.ttf'
				font_size: '20dp'
			Label:
				id: chgMultiPlayer2
				text: 'Player:'
				size_hint_x: .15
				color: (0,1,0,1)
				font_name: 'digital-7.regular.ttf'
				font_size: '20dp'
			Label:
				id: usrInfo
				size_hint_x: .35
				text: ''
				color: (0,1,0,1)
				font_name: 'digital-7.regular.ttf'
				font_size: '20dp'

