<PlacementHelp>:
	placeView: placeView
	BoxLayout:
		orientation: 'vertical'
		BoxLayout:
			padding: 5
			BoxLayout:
				orientation: 'vertical'
				Image:
					id: helpFigure
					source: 'placeHelp1.png'
					allow_stretch: True
			BoxLayout:
				padding: 5			
				ScrollView:
	                pos: self.pos
	                size: self.size
	                bar_width: 5
	                GridLayout:
	                    id: placeView
	                    cols: 1
	                    size_hint_y: None
	                    row_default_height: "100dp"
	                    row_force_default: True
	                    BoxLayout:
	                    	orientation: 'vertical'
		                    Label:
		                    	font_size: "25dp"
		                		text: 'Figure 1:'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
							Label:
		                		text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'Figure 1 shows the placement menu, here the player can see the ships that is available for placement.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
						BoxLayout:
							orientation: 'vertical'
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'In the block labeled A, an Icon of the ship appears on the left. Under the ship is the ship name and in brackets how many blocks on the ship occupies.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'On the right is two buttons labeled vertical(on top) and horizontal(on bottom), these buttons will select the orientation of the ship on the grid.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
						BoxLayout:
							orientation: 'vertical'
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'On tapping on one of the buttons the text in ellipse labeled B, will display the ship name, size and orientation for placement.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
							Label:
		                    	font_size: "25dp"
		                		text: 'Figure 2:'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
						BoxLayout:
							orientation: 'vertical'
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'Tap >> button to display figure 2. In figure 2 there are five ships placed on the grid. The Patrolboat(A) is placed with a vertical orientation and the Destroyer(B) horizontally.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'The Patrolboat(A) origin is on the bottom, meaning that the ship cannot be placed higher than row (B) on the grid. Else the player info area under the sonar will prompt to move the ship down.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
						BoxLayout:
							orientation: 'vertical'
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'The Destroyer(B) origin is on the left, meaning that the ship cannot be placed more right than column (8) on the grid. Else the player info area will prompt to move the ship left.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
							Label:
								text_size: self.size
		                		halign: 'left'
		                		valign: 'center'
		                		text: 'The ellipse labeled C will display the ship name, size and orientation.'
								font_name: 'digital-7.regular.ttf'
								color: (0,1,0,1)
						BoxLayout:
							orientation: 'vertical'
							Label:
								font_size: "25dp"
		                		text: 'Figure 3:'
								font_name: 'digital-7.regular.ttf'
